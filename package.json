    "@typescript-eslint/parser": "6.16.0",
    "@vitest/coverage-v8": "1.1.0",
    "@vitest/ui": "1.1.0",
    "eslint": "8.56.0",
    "npm-run-all": "4.1.5",
    "supertest": "6.3.4",
    "ts-prune": "0.10.3",
    "tsup": "8.0.1",
    "tsx": "4.7.0",
